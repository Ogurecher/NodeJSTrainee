{
  "name": "team-link",
  "version": "1.0.0",
  "description": "Provides a convenient interface to call all online team members via MS Teams",
  "main": "dist/src/index.js",
  "scripts": {
    "start": "npm run build && node dist/src/index.js",
    "lint": "eslint src/**/*.ts test/**/*-test.ts",
    "test": "npm run lint && mocha -r ts-node/register test/**/*-test.ts",
    "debug": "node --inspect-brk src/index.js",
    "watch": "npm test -- -w",
    "build": "tsc && tsc -p ./resources/client_scripts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ogurecher/Team-Link.git"
  },
  "author": "Developer Express Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ogurecher/Team-Link/issues"
  },
  "homepage": "https://github.com/Ogurecher/Team-Link#readme",
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "mocha": "^7.0.1",
    "proxyquire": "^2.1.3",
    "sinon": "^8.1.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@types/mocha": "^7.0.1",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "got": "^10.4.0",
    "nock": "^11.8.2"
  }
}
